{{#if loggedIn}}
<div class="container-fluid">
    <div class="card-group">
        <div class="col-xs-12 col-md-10 col-lg-8 m-auto">
            <div class="card border-dark">
                <div class="card-body">
                    <p>
                    <h3 class="card-title"> Hello <span class="text-warning">{{username}}</span>!</h3>
                    <em>Here are all of your fellow explorers. Click their names to see
                        their posts:</em>
                    </p>
                    {{#each allExplorers as |explorer|}}
                    {{>explorer-details-partials}}
                    {{/each}}
                </div>
            </div>
        </div>
        {{>continue-voyage-partials}}
    </div>
</div>
{{/if}}